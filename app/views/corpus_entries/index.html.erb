<h1>Listing corpus_entries</h1>

<table>
  <thead>
    <tr>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @corpus_entries.each do |corpus_entry| %>
      <tr>
        <td><%= corpus_entry.note %></td>
        <td><%= link_to 'Show', corpus_entry %></td>
        <td><%= link_to 'Edit', edit_corpus_entry_path(corpus_entry) %></td>
        <td><%= link_to 'Destroy', corpus_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Corpus entry', new_corpus_entry_path %>
